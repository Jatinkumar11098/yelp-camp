<%-layout('./layouts/boilerplate') %>
    <h1>Create a new campground!!</h1>
    <form action="/campgrounds" method="POST">
        <label for="title">Title</label>
        <input type="text" name="campground[title]" id="title" placeholder="title">
        <label for="location">Location</label>
        <input type="text" name="campground[location]" placeholder="location" id="location">
        <button>Submit</button>
    </form>
    <a href="/campgrounds">all campgounds</a>